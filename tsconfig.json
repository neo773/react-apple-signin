{
    "compilerOptions": {
      "target": "ES6",
      "outDir": "dist",
      "useDefineForClassFields": true,
      "lib": ["ES2020", "DOM", "DOM.Iterable"],
      "module": "ESNext",
      "skipLibCheck": true,
      "moduleResolution": "bundler",
      "declaration": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "react",
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
  }
  